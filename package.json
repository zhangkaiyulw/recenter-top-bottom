{
	"name": "recenter-top-bottom",
	"displayName": "Recenter Top Bottom",
	"description": "Reposition current lines at center top bottom.",
	"publisher": "yeannylam",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zhangkaiyulw/recenter-top-bottom.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:recenterTopBottom"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "recenterTopBottom",
				"title": "Recenter Top Bottom"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
